    cbi PORTA, 7
    cbi PORTA, 0

    sbi PORTA, 6
    sbi PORTA, 1

    cbi PORTA, 6
    cbi PORTA, 1

    sbi PORTA, 5
    sbi PORTA, 2

    cbi PORTA, 5
    cbi PORTA, 2

    sbi PORTA, 4
    sbi PORTA, 3

    cbi PORTA, 4
    cbi PORTA, 3

    sbi PORTA, 5
    sbi PORTA, 2

    cbi PORTA, 5
    cbi PORTA, 2

    sbi PORTA, 6
    sbi PORTA, 1

    cbi PORTA, 6
    cbi PORTA, 1

